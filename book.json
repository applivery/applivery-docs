{
    "plugins": [ "theme-api", "sitemap", "anchorjs" ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "sitemap": {
            "hostname": "https://docs.applivery.com/"
        },
        "anchorjs": {
            "selector": "h2,h3,h4,h5",
            "icon": "#",
            "placement": "left",
            "visible": "always",
            "class": "custom-class another-one",
            "truncate": 64,
            "ariaLabel": "Anchor"
        }
    }
}
